{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "google",
    "rules": {
        "no-unused-vars": "warn",
        "linebreak-style": "off",
        "max-len": "off",
        "spaced-comment": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "space-before-function-paren": "off",
        "quotes": "off"
    }
}